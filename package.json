{
  "name": "byoungz-next-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier '**/*.{js,jsx,ts,tsx,md,html,css,json,yml}' --ignore-path .gitignore --cache --write",
    "fix": "npm-run-all --parallel fix:js fix:css",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "fix:js": "next lint --fix",
    "lint:js": "next lint",
    "fix:css": "stylelint --fix src/styles/**/*.css",
    "lint:css": "stylelint src/styles/**/*.css",
    "type-check": "tsc --pretty",
    "prepare": "husky",
    "precommit": "npm run type-check && npm run fix",
    "test": "vitest"
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^1.4.0"
  }
}
