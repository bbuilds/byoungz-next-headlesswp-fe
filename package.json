{
  "name": "byoungz-next-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4444",
    "build": "next build",
    "start": "next start",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "format": "prettier '**/*.{js,jsx,ts,tsx,md,html,css,json,yml}' --ignore-path .gitignore --cache --write",
    "fix": "npm-run-all --parallel fix:js fix:css",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "fix:js": "next lint --fix",
    "lint:js": "next lint",
    "fix:css": "stylelint --fix src/styles/**/*.css",
    "lint:css": "stylelint src/styles/**/*.css",
    "type-check": "tsc --pretty",
    "prepare": "husky",
    "precommit": "npm run type-check && npm run fix",
    "test": "vitest",
    "generate": "plop",
    "codegen:wp": "DOTENV_CONFIG_PATH=./.env.local node -r dotenv/config  ./node_modules/.bin/graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@fontsource/grenze": "^5.0.20",
    "@fontsource/noto-sans": "^5.0.22",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "graphql": "^16.9.0",
    "html-react-parser": "^5.1.10",
    "isomorphic-unfetch": "^4.0.2",
    "next": "^14.2.15",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss-fluid-type": "^2.0.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.9.1",
    "@cloudflare/next-on-pages": "^1.13.5",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vitest": "^2.0.1",
    "vitest-axe": "^0.1.0"
  }
}
