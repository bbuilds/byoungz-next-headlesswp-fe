{
  "name": "byoungz-next-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier '**/*.{js,jsx,ts,tsx,md,html,css,json,yml}' --ignore-path .gitignore --cache --write",
    "fix": "npm-run-all --parallel fix:js fix:css",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "fix:js": "next lint --fix",
    "lint:js": "next lint",
    "fix:css": "stylelint --fix src/styles/**/*.css",
    "lint:css": "stylelint src/styles/**/*.css",
    "type-check": "tsc --pretty",
    "prepare": "husky",
    "precommit": "npm run type-check && npm run fix",
    "test": "vitest",
    "generate": "plop",
    "codegen:wp": "DOTENV_CONFIG_PATH=./.env.local node -r dotenv/config  ./node_modules/.bin/graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@fontsource/grenze": "^5.0.19",
    "@fontsource/noto-sans": "^5.0.21",
    "eslint-plugin-prettier": "^5.1.3",
    "graphql": "^16.8.1",
    "html-react-parser": "^5.1.10",
    "next": "14.1.4",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss-fluid-type": "^2.0.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.9.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0",
    "vitest-axe": "^0.1.0"
  }
}
